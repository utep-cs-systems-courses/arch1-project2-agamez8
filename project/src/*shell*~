student@systems-vm:~/arch1-project2-agamez8/project/src> make all
msp430-elf-gcc -mmcu=msp430g2553 -I../h   -c -o led.o led.c
msp430-elf-gcc -mmcu=msp430g2553 -I../h   -c -o switches.o switches.c
msp430-elf-gcc -mmcu=msp430g2553 -I../h   -c -o stateMachines.o stateMachines.c
msp430-elf-gcc -mmcu=msp430g2553 -I../h   -c -o main.o main.c
msp430-elf-gcc -mmcu=msp430g2553 -I../h   -c -o wdInterruptHandler.o wdInterruptHandler.c
msp430-elf-gcc -mmcu=msp430g2553 -I../h -L/opt/ti/msp430_gcc/include  -o main.elf buzzer.o led.o switches.o stateMachines.o main.o 2p-interrupt.o wdInterruptHandler.o ../lib/libTimer.a
student@systems-vm:~/arch1-project2-agamez8/project/src> make load
msp430loader.sh main.elf
check 
